  <script>
    getSubscriptionDetailsStarterApi().then(data => {
      const packageData = data.data[0];
      document.querySelector(".pkg_name_1 h3").textContent = packageData.packageName;
      // document.querySelector(".original-price").textContent = `${packageData.originalAnnualRate}`;
      document.querySelector(".discounted-price-1").textContent = `${packageData.monthlyRate}`;
      const benefitsList = document.querySelector(".benifits-1");
      packageData.features.forEach(feature => {
        const li = document.createElement("li");
        const p = document.createElement("p");
        p.textContent = feature;
        li.appendChild(p);
        benefitsList.appendChild(li);
      });
    });
  </script>
  <script>
    getSubscriptionDetailsPremiumApi().then(data => {
      const packageData = data.data[0];
      document.querySelector(".pkg_name_2 h3").textContent = packageData.packageName;
      // document.querySelector(".original-price").textContent = `${packageData.originalAnnualRate}`;
      document.querySelector(".discounted-price-2").textContent = `${packageData.monthlyRate}`;
      const benefitsList = document.querySelector(".benifits-2");
      packageData.features.forEach(feature => {
        const li = document.createElement("li");
        const p = document.createElement("p");
        p.textContent = feature;
        li.appendChild(p);
        benefitsList.appendChild(li);
      });
    });
  </script>
  <script>
    getSubscriptionDetailsYearlyStarter().then(data => {
      const packageData = data.data[0];
      document.querySelector(".pkg_name_3 h3").textContent = packageData.packageName;
      // document.querySelector(".original-price").textContent = `${packageData.originalAnnualRate}`;
      document.querySelector(".discounted-price-3").textContent = `${packageData.annualRate}`;
      const benefitsList = document.querySelector(".benifits-3");
      packageData.features.forEach(feature => {
        const li = document.createElement("li");
        const p = document.createElement("p");
        p.textContent = feature;
        li.appendChild(p);
        benefitsList.appendChild(li);
      });
    });
  </script>
